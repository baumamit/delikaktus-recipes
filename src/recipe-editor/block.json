{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/recipe-editor",
	"version": "0.1.0",
	"title": "Recipe Editor",
	"category": "widgets",
	"attributes": {
		"blockHeading": { "type": "string", "default": "Ingredients" },
		"unitSystem": { "type": "string", "default": "metric" },
		"ingredients": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object",
				"properties": {
					"unitType": { "type": "string", "default": "" },
					"quantity": { "type": "string", "default": "" },
					"quantityFraction": { "type": "string", "default": "" },
					"unitChoice": { "type": "string", "default": "" },
					"name": { "type": "string", "default": "" }
				}
			}
		}
	},
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "recipe-editor",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
